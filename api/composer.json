{
    "name": "deepydee/auction",
    "version": "1.0.0",
    "type": "project",
    "license": "MIT",
    "config": {
      "process-timeout": 0,
      "sort-packages": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "php-di/php-di": "^7.0",
        "slim/psr7": "dev-master",
        "slim/slim": "^4.0",
        "symfony/console": "7.3.x-dev"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Test\\": "tests/"
      }
    },
    "scripts": {
      "app": "php bin/app.php --ansi --no-interaction"
    }
}
